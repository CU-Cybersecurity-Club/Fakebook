DROP TABLE IF EXISTS users;
DROP TABLE IF EXISTS posts;
CREATE TABLE IF NOT EXISTS users (username TEXT, password TEXT);
CREATE TABLE IF NOT EXISTS posts (author TEXT, posted TEXT, content TEXT);
